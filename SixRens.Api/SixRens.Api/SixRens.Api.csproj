<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Authors>YiJingFramework</Authors>
    <Company>YiJingFramework</Company>
    <Product>SixRens.Api</Product>
    <PackageId>SixRens.Api</PackageId>
    <Version>1.15.0</Version>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <RepositoryUrl>https://github.com/YiJingFramework/sixrens-api.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <NeutralLanguage>zh-Hans</NeutralLanguage>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="YiJingFramework.Core" Version="4.1.0" />
    <PackageReference Include="YiJingFramework.FiveElements" Version="3.1.0" />
    <PackageReference Include="YiJingFramework.StemsAndBranches" Version="3.0.2" />
  </ItemGroup>

</Project>
